@model ErrorLoggerModel.UserModel

@{
    ViewBag.Title = "AssignUserToApp";
}

<h2 class="title-head">Assign User To App</h2>

<h3>Are you sure you want to Assign this User?</h3>

<div>
    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Assign" class="btn btn-default" style="background: #26A69A; color: #fff;"/> 
            <button class="btn btn-primary" type="button">
                @Html.ActionLink("Cancel", "ViewApplications")
            </button> 
        </div>
    }
</div>
